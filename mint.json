{
  "name": "mint-tutorial",
  "mint-version": "0.18.0 <= v < 1.0.0",
  "application": {
    "head": "assets/head.html",
    "title": "Mint Tutorial"
  },
  "source-directories": [
    "source"
  ],
  "external": {
    "javascripts": [
      "assets/highlight.min.js",
      "assets/highlight.mint.min.js"
    ]
  },
  "dependencies": {
    "mint-ui": {
      "repository": "https://github.com/mint-lang/mint-ui",
      "constraint": "7.0.0 <= v < 8.0.0"
    },
    "mint-codemirror": {
      "repository": "https://github.com/mint-lang/mint-codemirror",
      "constraint": "9.0.0 <= v < 10.0.0"
    },
    "mint-tabler-icons": {
      "repository": "https://github.com/mint-lang/mint-tabler-icons",
      "constraint": "master:1.0.0"
    }
  }
}
